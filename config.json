{
  "Sampling": {
    "low": -40,
    "high": 100,
    "step": 5,
    "interval": 3
  },
  "Persistence": {
    "interval": 15,
    "file": "/tmp/temp/data_file"
  },
  "Notify": {
    "file": "/tmp/temp/notify"
  },
