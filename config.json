    },
    {
      "name": "temp3",
      "file": "/tmp/sys/class/hwmon/hwmon2/device/temp1_input"
    },
    {
      "name": "temp4",
      "file": "/tmp/sys/class/hwmon/hwmon3/device/temp1_input"
    },
    {
      "name": "temp5",
      "file": "/tmp/sys/class/hwmon/hwmon4/device/temp1_input"
    },
    {
      "name": "temp6",
      "file": "/tmp/sys/class/hwmon/hwmon5/device/temp1_input"
    },
    {
      "name": "temp7",
      "file": "/tmp/sys/class/hwmon/hwmon6/device/temp1_input"
    },
    {
      "name": "temp8",
      "file": "/tmp/sys/class/hwmon/hwmon7/device/temp1_input"
